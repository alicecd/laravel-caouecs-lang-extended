{
  "name": "novius/caouecs-lang-extended",
  "description": "Generate default languages files for Laravel",
  "keywords": [
    "laravel",
    "languages"
  ],
  "homepage": "http://novius.com",
  "require": {
    "php": ">=5.6.4",
    "caouecs/laravel-lang": "~3.0",
    "laravel/framework": "5.4.*",
    "mockery/mockery": "^0.9.9"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "orchestra/testbench": "~3.0",
    "friendsofphp/php-cs-fixer": "~2.5.0"
  },
  "autoload": {
    "psr-4": {
      "Novius\\Caouecs\\Lang\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Novius\\CaouecsLangExtended\\Tests\\": "tests"
    },
    "classmap": [
      "tests"
    ]
  },
  "scripts": {
    "lint": [
      "php-cs-fixer-v2 fix --config .php_cs -vv --diff --allow-risky=yes --dry-run"
    ],
    "test": [
      "phpunit"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
